SELECT order_id
FROM   courier_actions
WHERE  date_part('year', time) = 2022
   and date_part('month', time) = 8
   and date_part('day', time) between 1
   and 31
   and action like 'accept_order'
ORDER BY order_id asc




SELECT order_id
FROM   courier_actions
WHERE  date_part('year', time) = 2022
   and date_part('month', time) = 8
  
   and action like 'accept_order'
ORDER BY order_id asc



SELECT courier_id
FROM   couriers
WHERE  date_part('year', birth_date) between 1990
   and 1995
ORDER BY courier_id
